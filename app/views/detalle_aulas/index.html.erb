<p id="notice"><%= notice %></p>
<div class="container">
<h2 class="text-center text-muted">Detalle Aulas</h2>
</div>

<table>
  <thead>
    <tr>
      <th>Observacion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @detalle_aulas.each do |detalle_aula| %>
      <tr>
        <td><%= detalle_aula.observacion %></td>
        <td><%= link_to 'Ver', detalle_aula %></td>
        <td><%= link_to 'Editar', edit_detalle_aula_path(detalle_aula) %></td>
        <td><%= link_to 'Eliminar', detalle_aula, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo', new_detalle_aula_path %>
