<p id="notice"><%= notice %></p>
<div class="container">
  <h2 class="text-center text-muted">Registro de Carrera</h2>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Carrera</th>
      <th>AÃ±o</th>
      <th>Semestre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registro_carreras.each do |registro_carrera| %>
      <tr>
        <td><%= registro_carrera.nombreCarrera %></td>
        <td><%= registro_carrera.anho %></td>
        <td><%= registro_carrera.semestre %></td>
