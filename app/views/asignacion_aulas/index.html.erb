<p id="notice"><%= notice %></p>

<h1>Asignacion Aulas</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Observacion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @asignacion_aulas.each do |asignacion_aula| %>
      <tr>
        <td><%= asignacion_aula.facultads_id %></td>
        <td><%= asignacion_aula.registro_carreras_id %></td>
        <td><%= asignacion_aula.anho_carrera_id %></td>
        <td><%= asignacion_aula.observacion %></td>
        <td><%= link_to 'Show', asignacion_aula %></td>
        <td><%= link_to 'Edit', edit_asignacion_aula_path(asignacion_aula) %></td>
        <td><%= link_to 'Destroy', asignacion_aula, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


<button type="button" class="btn btn-info"><%= link_to 'New Asignacion Aula', new_asignacion_aula_path %></button>